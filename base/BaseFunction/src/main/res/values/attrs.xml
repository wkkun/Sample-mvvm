<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AspectRatioImageView">
        <attr name="aspectRatio" format="float" />
        <attr name="aspectRatioEnabled" format="boolean" />
        <attr name="dominantMeasurement">
            <enum name="width" value="0" />
            <enum name="height" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="AspectRatioFrameLayout">
        <attr name="fl_aspectRatio" format="float" />
        <attr name="fl_aspectRatioEnabled" format="boolean" />
        <attr name="fl_dominantMeasurement">
            <enum name="width" value="0" />
            <enum name="height" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="CustomTitleView">
        <attr name="title" format="string" />
        <attr name="showCloseIcon" format="boolean" />
        <attr name="showDivider" format="boolean" />
        <attr name="dividerColor" format="color" />
    </declare-styleable>


</resources>